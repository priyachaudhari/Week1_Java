class Solution {
    public int findComplement(int num) {
        
        int sum = 0;
        
        while(sum < num){
            sum = (sum << 1) | 1; 
        }
        
        return sum - num;
        
//         if(num == 0){
//             return 0;
//         }
//         else if(num == 1){
//             return 1;
//         }
        
//         if(num > 1){
//             num = num ^ 7;
//             System.out.println(num);
//         }
        
//         return num;
    }
}
